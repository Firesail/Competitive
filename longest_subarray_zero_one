#include <iostream>
using namespace std;

void cal(int a[], int n)
{
    int i, diff[n], pos[n]={0}, max=0, l, r, x=0, y=0;
    
    for(i=0;i<n;i++)
    {
        if(a[i]==0) x++;
        else y++;
        
        diff[i] = x-y;
        
        pos[diff[i]] = i;
    }
    
    if(x==0 || y==0)
        {cout<<"\nNo Subaaray"; return;}
    
    for(i=0;i<n-3;i++)
    {
        x=i;
        if(a[i]==0)
            y = pos[diff[i]-1];
        else
            y = pos[diff[i]+1];
        
        if(y-x  > max)
            {
                max = y-x;
                l=x;
                r=y;
            }    
    }
    
    cout<<"\n"<<l<<"  "<<r;
}

int main() {

	int T;
	cin>>T;
	while(T--)
	{
		int n;
		cin>>n;
		int a[n];
		for(int i=0;i<n;i++)
		    cin>>a[i];
		cal(a, n);    
	}
	
	return 0;
}
